generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Article {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  content   String
  imgUrl    String
  createdAt DateTime @default(now())
}

model Images {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  url  String
}
